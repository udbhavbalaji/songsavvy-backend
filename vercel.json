{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "memory": 1024,
      "maxDuration": 10,
      "excludeFiles": [
        ".git/**",
        ".gitignore",
        ".env",
        "README.md"
      ]
    }
  },
  "routes": [
    {
      "src": "/api/predict/(?<songId>[^/]+)",
      "dest": "/api/predict.py"
    }
  ]
}
